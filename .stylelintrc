{
    "extends": "stylelint-config-standard",
    "rules": {
        "at-rule-no-unknown": [true, {
            "ignoreAtRules": ["function", "if", "each", "include", "mixin"]
        }],
        "selector-type-no-unknown": [true, {
            "ignore": ["custom-elements", "default-namespace"]
        }],
        "no-descending-specificity": null
    }
} 